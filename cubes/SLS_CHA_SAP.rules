#Calcul du prix unitaire promo
['PROMO_UNIT'] = C: If( ELISANC('SLS_CUSTOMER', 'IC1 - E00095', !SLS_CUSTOMER)=1 % ELISANC('SLS_CUSTOMER', 'IC1 - E00070', !SLS_CUSTOMER) =1 ,['PROMO']\['SLS_VOL'],['PROMO']\['PROMO_VOL']);

#Calcul des prix unitaires pour les niveaux consolidés
[] = C: IF(DB('}ElementAttributes_SLS_CHA_SAP_IND', !SLS_CHA_SAP_IND, 'Type_Indicateur')@= 'Prix_Unit',
		DB('SLS_CHA_SAP', !'1_G_PHASE', !'1_G_PERIOD', !'1_G_SALES_ORG', !'1_G_DISTRIB_CHANNEL', !'1_G_VOLUME_UNIT', !SLS_CUSTOMER, !'1_G_MATERIAL', !SLS_SALES_GROUP, !SLS_SHIP_POINT, !SLS_SHIP_PLANT, !SLS_PROD_PLANT, !SLS_VENDOR, !SLS_PAYER, !SLS_SOLD_TO, !SLS_INCOTERM, !SLS_Q_HQ, AttrS('SLS_CHA_SAP_IND',!SLS_CHA_SAP_IND, 'Valo_Associée'))\['SLS_VOL'],
        CONTINUE );


Feeders;



